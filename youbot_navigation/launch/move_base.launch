<?xml version="1.0"?>
<launch>
  <master auto="start"/>
  
  <!-- launch generated map -->
  <node pkg="map_server" type="map_server" name="slalom_walls_server" output="screen" 
    args="$(find youbot_navigation)/maps/slalom_map2.yaml" respawn="false">
  </node>

  <!--- Run AMCL -->
  <include file="$(find amcl)/examples/amcl_diff.launch" />
  <!-- <include file="$(find youbot_navigation)/launch/amcl_omni.launch" /> -->

  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
    <rosparam file="$(find youbot_navigation)/cost_params/costmap_common_params.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find youbot_navigation)/cost_params/costmap_common_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find youbot_navigation)/cost_params/local_costmap_params.yaml" command="load" />
    <rosparam file="$(find youbot_navigation)/cost_params/global_costmap_params.yaml" command="load" />
    <rosparam file="$(find youbot_navigation)/cost_params/base_local_planner_params.yaml" command="load" />
    <!-- <param name="use_sim_time" value="true" /> -->
  </node>
  
</launch>